    .setColumn {
      display: grid;
      grid-template-columns: 1fr;
    }

    .setColumn-p {
      display: grid;
      grid-template-columns: 1.25fr 1.25fr 0.5fr !important;
      padding-bottom: 0.5em !important;
      margin-bottom: 0.5em !important;

      border-bottom: 1px solid #0D6EFD;
      
    }

    .setColumn-p-lesson {
      display: grid;
